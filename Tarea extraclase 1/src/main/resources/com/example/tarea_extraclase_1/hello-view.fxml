<VBox alignment="CENTER" prefHeight="524.0" prefWidth="502.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tarea_extraclase_1.HelloController">
